module Input;

open import String;

open import Bool;

open import Pair;

open import IO;

-- Not needed but useful for testing
open import Prelude;

bool-to-str : Bool → String;
bool-to-str true ≔ "True";
bool-to-str false ≔ "False";

--------------------------------------------------------------------------------
-- Main
--------------------------------------------------------------------------------

fst-of-pair : Action;
fst-of-pair ≔ (put-str "fst (True, False) = ")
               >> put-str-ln (bool-to-str (fst Bool Bool (mkPair true false)));

main : Action;
main ≔ fst-of-pair;

end;
