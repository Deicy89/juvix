module Input;

import String;
open String;

import Bool;
open Bool;

import Pair;
open Pair;

import IO;
open IO;

-- Not needed but useful for testing
import Prelude;
open Prelude;

bool-to-str : Bool → String;
bool-to-str true ≔ "True";
bool-to-str false ≔ "False";

--------------------------------------------------------------------------------
-- Main
--------------------------------------------------------------------------------

fst-of-pair : Action;
fst-of-pair ≔ (put-str "fst (True, False) = ")
               >> put-str-ln (bool-to-str (fst Bool Bool (mkPair Bool Bool true false)));

main : Action;
main ≔ fst-of-pair;

end;
